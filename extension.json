{
    "manifest_version":2,
    "name":"SvgEditor",
    "version":"0.1.0",
    "author":[
        "[https://github.com/simontaurus Simon Stier]"
    ],
    "url":"https://github.com/OpenSemanticLab/mediawiki-extensions-SvgEditor",
    "descriptionmsg":"svg-editor-desc",
    "namemsg":"svg-editor-name",
    "license-name":"AGPL-3.0",
    "type":"editor",
    "requires":{
        "MediaWiki":">= 1.31"
    },
    "MessagesDirs":{
        "SvgEditor":[
            "i18n"
        ]
    },
    "ResourceModules":{
        "ext.SvgEditor":{
            "localBasePath":"modules/ext.SvgEditor",
            "remoteExtPath":"SvgEditor/modules/ext.SvgEditor",
            "styles":[
                "SvgEditor.css"
            ],
            "scripts":[
                "SvgEditor.js"
            ],
            "position":"bottom"
        }
    },
    "AutoloadClasses":{
        "SvgEditor":"includes/SvgEditor.php"
    },
    "Hooks":{
        "BeforePageDisplay":"SvgEditor::onBeforePageDisplay"
    }
}
